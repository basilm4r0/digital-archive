@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix sabil: <http://sabil.ethz.ch/ns/terms/> .

<http://sabil.ethz.ch/ns/terms/> a owl:Ontology ;
    dcterms:title "Sabil Ontology" ;
    dcterms:description "Specific terms used for the Sabil project" .

sabil:documentProcessingStatus a rdf:Property ;
    rdfs:label "Document Processing Status" ;
    rdfs:comment "Describes the current status of the document, should be PROCESSED, TO_BE_PROCESSED, or FAILURE" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

sabil:submittedBy a rdf:Property ;
    rdfs:label "Submitted By" ;
    rdfs:comment "The user who submitted the document" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

sabil:documentProcessingFailureMessage a rdf:Property ;
    rdfs:label "Document Processing Failure Message" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

sabil:GeneratedPage a rdfs:Class ;
    rdfs:label "Generated Page" ;
    rdfs:comment "Page item generated by the OCR Bot" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

sabil:ProcessedMedia a rdfs:Class ;
    rdfs:label "Processed Media" ;
    rdfs:comment "Media processed, OCRized and cleaned by the OCR Bot" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .
